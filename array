//to find the largest number in array

#include<iostream>
using namespace std;

int main(void){
int n,max=0;
cout<<"enter the no. of elements you want in array";
cin>>n;
cout<<"enter the elements"<<endl;
int arr[n];
for(int i=0;i<n;i++){
cin>>arr[i];
}
for(int i=0;i<n;i++){
if(arr[i]>max){
max=arr[i];
}

}
cout<<"max element"<<max<<endl;
}
